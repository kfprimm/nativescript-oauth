<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo">
  <StackLayout>
    <Label text="How do you want to log in?" class="login-title"/>
    <Button class="btn-login btn-login-g" text="Google Account" tap="{{ onGoogleTap }}"/>
    <Button class="btn-login btn-login-ms" text="Microsoft Account" tap="{{ onMicrosoftTap }}"/>

    <Button text="Get data" tap="{{ onTapGetData }}" />
  </StackLayout>
</Page>
